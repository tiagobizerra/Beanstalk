option_settings:
  - namespace: aws:ec2:vpc
    option_name: VPCId
    value: vpc-a1dbecc4

  - namespace: aws:ec2:vpc
    option_name: Subnets
    value: subnet-5944863d

  - namespace: aws:elb:policies
    option_name: ConnectionDrainingEnabled
    value: true
    option_name: ConnectionDrainingTimeout
    value: 300
    option_name: ConnectionSettingIdleTimeout
    value: 301
    option_name: LoadBalancerPorts
    value: all
    option_name: SticknessPolicy
    value: false

  - namespace: aws:elb:healthcheck
    option_name: HealthyThreshold
    value: 4
    option_name: Interval
    value: 12
    option_name: Timeout
    value: 4
    option_name: UnhealthyThreshold
    value: 6 

Resources:
    AWSEBLoadBalancer:
        Type: AWS::ElasticLoadBalancing::LoadBalancer
        Properties:
            AccessLoggingPolicy:
                EmitInterval: 5
                Enabled: true
                S3BucketName: aws-tiaglima-logs